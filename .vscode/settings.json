{
  "[rust]": {
    "editor.formatOnSave": true
  },
  "[toml]": {
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.formatOnSave": true
  },
  "files.associations": {
    "*.snap": "markdown",
    "*.snap.new": "markdown"
  },
  "cSpell.words": [
    "Finalizer",
    "hasher",
    "importee",
    "napi",
    "oxlint",
    "oxlintignore",
    "Renamer",
    "repr",
    "rolldown",
    "rollup",
    "rustc",
    "smallvec"
  ],
  "git.ignoreLimitWarning": true,
  "json.schemas": [
    {
      "fileMatch": ["test.config.json"],
      "url": "./crates/rolldown_testing/test.config.scheme.json"
    }
  ]
}
