## main.js

```js
import { strictEqual } from "node:assert";

//#region state.js
const state = {};

//#endregion
//#region helpers.js
function getConfig(key, defaultValue) {
	const value = state.config?.[key];
	if (value !== void 0) return value;
	return defaultValue;
}

//#endregion
//#region shared.js
const TIMEOUT = getConfig("TIMEOUT", 3e5);
const MAX_RETRIES = getConfig("MAX_RETRIES", 3);

//#endregion
//#region main.js
state.config = { TIMEOUT: 5e3 };
strictEqual(TIMEOUT, 3e5);
strictEqual(MAX_RETRIES, 3);
strictEqual(getConfig("TIMEOUT", 999), 5e3);

//#endregion
```
