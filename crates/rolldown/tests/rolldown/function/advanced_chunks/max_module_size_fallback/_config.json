{
  "config": {
    "advancedChunks": {
      // Only `size-14.js` will be included in the chunk
      "maxModuleSize": 20,
      "groups": [
        {
          // `size-14.js` and `size-23.js` will be captured
          "test": "size-.*\\.js",
          "name": "smaller-than-20"
        }
      ]
    }
  }
}