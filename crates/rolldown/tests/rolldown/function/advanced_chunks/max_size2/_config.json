{
  // Unsplitable max size shouldn't make the group invalid/ignored
  "config": {
    "advancedChunks": {
      "groups": [
        {
          // `size-15.js`, `size-20` and `size-41.js` will be captured
          "test": "size-.*\\.js",
          "name": "14max-size",
          // Smaller than the smallest module `size-15.js`
          "maxSize": 14
        }
      ]
    }
  },
  "hiddenRuntimeModule": false
}