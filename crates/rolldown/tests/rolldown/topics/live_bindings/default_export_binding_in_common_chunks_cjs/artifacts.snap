"use strict";

const node_assert = __toESM(require("node:assert"));

//#region shared.js
let count = 0;
function reset() {
	count = 0;
}
function inc() {
	count += 1;
}

//#endregion
//#region async-entry.js
reset();
node_assert.strictEqual(count, 0);
node_assert.strictEqual(count, count);
inc();
node_assert.strictEqual(count, 1);
node_assert.strictEqual(count, count);

//#endregion
