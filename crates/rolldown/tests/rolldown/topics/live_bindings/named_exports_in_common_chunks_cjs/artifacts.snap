assert.strictEqual(require_shared.count, 0);
assert.strictEqual(require_shared.count, require_shared.count);
require_shared.inc();
assert.strictEqual(require_shared.count, 1);
assert.strictEqual(require_shared.count, require_shared.count);
require_shared.inc();
assert.strictEqual(require_shared.count, 2);
assert.strictEqual(require_shared.count, require_shared.count);

