            }
          }
        },
        "extensions": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        },
        "extensionAlias": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "array",
            "prefixItems": [
              {
                "type": "string"
              },
              {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            ],
            "minItems": 2,
            "maxItems": 2
