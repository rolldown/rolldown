{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "ignorePaths": [
    "**/node_modules",
    "**/dist",
    "*.snap",
    "*.node",
    "*.tsbuildinfo",
    "*.json",
    "*.toml",
    "*.css",
    "*.svg",
    "*.data",
    "*.d.ts",
    "/rollup",
    "/packages/rollup-tests/test",
    "/target",
    "/temp",
    "/tmp",
    "/packages/rolldown/npm",
    "scripts/snap-diff/**/*.{txt,md}",
    "packages/rolldown/src/binding.js",
    "/CHANGELOG.md",
    "/packages/rolldown/src/rollup.d.ts",
    "/crates/rolldown/tests/esbuild/splitting/ignore-test.md",
    "/crates/rolldown_utils/src/light_guess.rs",
    "/pnpm-lock.yaml",
    "*.wasm"
  ],
  "languageSettings": [
    {
      // VSCode languageId. i.e. typescript, java, go, cpp, javascript, markdown, latex
      // * will match against any file type.
      "languageId": "*",
      // Language locale. i.e. en-US, de-AT, or ru. * will match all locales.
      // Multiple locales can be specified like: "en, en-US" to match both English and English US.
      "locale": "*",
      // To exclude patterns, add them to "ignoreRegExpList"
      "ignoreRegExpList": ["/import\\s.*/"],
      // List of dictionaries to enable by name in `dictionaryDefinitions`
      "dictionaries": [],
      // Dictionary definitions can also be supplied here. They are only used if "languageId" and "locale" match.
      "dictionaryDefinitions": []
    }
  ],
