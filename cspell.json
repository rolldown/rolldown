    "hashbang",
    "hasher",
    "iife",
    "importee",
    "importstar",
    "insta",
    "libc",
    "Lukas",
    "metafile",
    "metas",
    "mimalloc",
    "minifier",
    "napi",
    "nocheck",
    "nodenext",
    "onwarn",
